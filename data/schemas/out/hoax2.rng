         <ref name="tei_hoaxatt.personal.attributes"/>
         <ref name="tei_hoaxatt.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_hoaxsurname">
      <element name="surname">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains a family (inherited) name, as opposed to a given, baptismal, or nick name. [13.2.1. Personal Names]</a:documentation>
         <ref name="tei_hoaxmacro.phraseSeq"/>
         <ref name="tei_hoaxatt.global.attributes"/>
         <ref name="tei_hoaxatt.personal.attributes"/>
         <ref name="tei_hoaxatt.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_hoaxforename">
      <element name="forename">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains a forename, given or baptismal name. [13.2.1. Personal Names]</a:documentation>
         <ref name="tei_hoaxmacro.phraseSeq"/>
         <ref name="tei_hoaxatt.global.attributes"/>
         <ref name="tei_hoaxatt.personal.attributes"/>
         <ref name="tei_hoaxatt.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_hoaxplaceName">
      <element name="placeName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains an absolute or relative place name. [13.2.3. Place Names]</a:documentation>
         <ref name="tei_hoaxmacro.phraseSeq"/>
         <ref name="tei_hoaxatt.datable.attributes"/>
         <ref name="tei_hoaxatt.editLike.attributes"/>
         <ref name="tei_hoaxatt.global.attributes"/>
         <ref name="tei_hoaxatt.personal.attributes"/>
         <ref name="tei_hoaxatt.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_hoaxsettlement">
      <element name="settlement">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains the name of a settlement such as a city, town, or village identified as a single geo-political or administrative unit. [13.2.3. Place Names]</a:documentation>
         <ref name="tei_hoaxmacro.phraseSeq"/>
         <ref name="tei_hoaxatt.global.attributes"/>
         <ref name="tei_hoaxatt.naming.attributes"/>
         <ref name="tei_hoaxatt.typed.attributes"/>
         <ref name="tei_hoaxatt.datable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_hoaxgeo">
      <element name="geo">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(geographical coordinates) contains any expression of a set of geographic coordinates, representing a point, line, or area on the surface of the earth in some notation. [13.3.4.1. Varieties of Location]</a:documentation>
         <text/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_hoax-geo-geo-constraint-assert-6">
            <rule context="tei:geo">
               <s:assert xmlns:s="http://purl.oclc.org/dsdl/schematron"
                         xmlns="http://www.tei-c.org/ns/1.0"
                         test="matches(., '^\S+ \S+$')">Whitespace error in &lt;geo&gt;
                element</s:assert>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_hoax-geo-geo-constraint-assert-7">
            <rule context="tei:geo">
               <s:assert xmlns:s="http://purl.oclc.org/dsdl/schematron"
                         xmlns="http://www.tei-c.org/ns/1.0"
                         test="every $geopart in tokenize(., ' ') satisfies $geopart castable as xs:double">Some part of <s:value-of select="."/> is not numeric. Is there a comma inside?</s:assert>
            </rule>
         </pattern>
         <ref name="tei_hoaxatt.global.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_hoaxlistPlace">
      <element name="listPlace">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(list of places) contains a list of places, optionally followed by a list of relationships (other than containment) defined amongst them. [2.2.7. The Source Description 13.3.4. Places]</a:documentation>
         <group>
      
            <zeroOrMore>
               <ref name="tei_hoaxmodel.headLike"/>
            </zeroOrMore>
      
      
            <oneOrMore>
               <choice>
                  <ref name="tei_hoaxmodel.placeLike"/>
                  <ref name="tei_hoaxlistPlace"/>
               </choice>
            </oneOrMore>
      
      
            <zeroOrMore>
               <empty/>
            </zeroOrMore>
      
         </group>
         <ref name="tei_hoaxatt.global.attributes"/>
         <ref name="tei_hoaxatt.typed.attributes"/>
         <ref name="tei_hoaxatt.sortable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_hoaxlocation">
      <element name="location">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">defines the location of a place as a set of geographical coordinates, in terms of other named geo-political entities, or as an address. [13.3.4. Places]</a:documentation>
         <zeroOrMore>
            <choice>
        
               <ref name="tei_hoaxmodel.labelLike"/>
               <ref name="tei_hoaxmodel.placeNamePart"/>
               <ref name="tei_hoaxmodel.offsetLike"/>
               <ref name="tei_hoaxmodel.measureLike"/>
        
               <ref name="tei_hoaxmodel.noteLike"/>
               <ref name="tei_hoaxmodel.biblLike"/>
            </choice>
         </zeroOrMore>
         <ref name="tei_hoaxatt.global.attributes"/>
         <ref name="tei_hoaxatt.typed.attributes"/>
         <ref name="tei_hoaxatt.datable.attributes"/>
         <ref name="tei_hoaxatt.editLike.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_hoaxplace">
      <element name="place">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains data about a geographic location [13.3.4. Places]</a:documentation>
         <group>
      
            <zeroOrMore>
               <ref name="tei_hoaxmodel.headLike"/>
            </zeroOrMore>
      
            <choice>
        
          
               <zeroOrMore>
                  <ref name="tei_hoaxmodel.pLike"/>
               </zeroOrMore>
          
        
        
               <zeroOrMore>
                  <choice>
                     <ref name="tei_hoaxmodel.labelLike"/>
                     <ref name="tei_hoaxmodel.placeStateLike"/>
                     <ref name="tei_hoaxmodel.eventLike"/>
                  </choice>
               </zeroOrMore>
        
            </choice>
      
            <zeroOrMore>
               <choice>
                  <ref name="tei_hoaxmodel.noteLike"/>
                  <ref name="tei_hoaxmodel.biblLike"/>
          
          
          
               </choice>
            </zeroOrMore>
      
      
            <zeroOrMore>
               <choice>
                  <ref name="tei_hoaxmodel.placeLike"/>
                  <ref name="tei_hoaxlistPlace"/>
               </choice>
            </zeroOrMore>
      
         </group>
         <ref name="tei_hoaxatt.global.attributes"/>
         <ref name="tei_hoaxatt.typed.attributes"/>
         <ref name="tei_hoaxatt.editLike.attributes"/>
         <ref name="tei_hoaxatt.sortable.attributes"/>
         <empty/>
      </element>
   </define>
   <start>
      <choice>
         <ref name="tei_hoaxTEI"/>
      </choice>
   </start>
</grammar>
