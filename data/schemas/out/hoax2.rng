         <ref name="tei_hoaxatt.typed.attributes"/>
         <ref name="tei_hoaxatt.sortable.attributes"/>
         <ref name="tei_hoaxatt.docStatus.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_hoaxl">
      <element name="l">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(verse line) contains a single, possibly incomplete, line of verse. [3.12.1. Core Tags for Verse 3.12. Passages of Verse or Drama 7.2.5. Speech Contents]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="tei_hoaxmodel.gLike"/>
               <ref name="tei_hoaxmodel.phrase"/>
               <ref name="tei_hoaxmodel.inter"/>
               <ref name="tei_hoaxmodel.global"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tei_hoax-l-abstractModel-structure-l-constraint-report-7">
            <rule context="tei:l">
               <report test="ancestor::tei:l[not(.//tei:note//tei:l[. = current()])]">
