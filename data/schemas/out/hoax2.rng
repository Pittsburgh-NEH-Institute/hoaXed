                     <zeroOrMore>
                        <choice>
                           <ref name="tei_hoaxmodel.global"/>
                           <ref name="tei_hoaxmodel.divGenLike"/>
                        </choice>
                     </zeroOrMore>
              
                  </group>
               </oneOrMore>
          
          
          
               <oneOrMore>
                  <group>
              
                     <ref name="tei_hoaxmodel.div1Like"/>
              
              
                     <zeroOrMore>
                        <choice>
                           <ref name="tei_hoaxmodel.global"/>
                           <ref name="tei_hoaxmodel.divGenLike"/>
                        </choice>
                     </zeroOrMore>
              
                  </group>
               </oneOrMore>
          
          
               <group>
                  <oneOrMore>
                     <group>
              
                        <ref name="tei_hoaxmodel.common"/>
              
              
                        <zeroOrMore>
                           <ref name="tei_hoaxmodel.global"/>
                        </zeroOrMore>
              
                     </group>
                  </oneOrMore>
            
                  <optional>
                     <choice>
                
                
                        <oneOrMore>
                           <group>
                    
                              <ref name="tei_hoaxmodel.divLike"/>
                    
                    
                              <zeroOrMore>
                                 <choice>
